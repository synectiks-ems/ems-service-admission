<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity StudentExamReport.
    -->
    <changeSet id="20190523093814-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="academic_exam_setting_id"
                                 baseTableName="student_exam_report"
                                 constraintName="fk_student_exam_report_academic_exam_setting_id"
                                 referencedColumnNames="id"
                                 referencedTableName="academic_exam_setting"/>

        <addForeignKeyConstraint baseColumnNames="student_id"
                                 baseTableName="student_exam_report"
                                 constraintName="fk_student_exam_report_student_id"
                                 referencedColumnNames="id"
                                 referencedTableName="student"/>

        <addForeignKeyConstraint baseColumnNames="section_id"
                                 baseTableName="student_exam_report"
                                 constraintName="fk_student_exam_report_section_id"
                                 referencedColumnNames="id"
                                 referencedTableName="section"/>

        <addForeignKeyConstraint baseColumnNames="subject_id"
                                 baseTableName="student_exam_report"
                                 constraintName="fk_student_exam_report_subject_id"
                                 referencedColumnNames="id"
                                 referencedTableName="subject"/>

        <addForeignKeyConstraint baseColumnNames="department_id"
                                 baseTableName="student_exam_report"
                                 constraintName="fk_student_exam_report_department_id"
                                 referencedColumnNames="id"
                                 referencedTableName="department"/>

        <addForeignKeyConstraint baseColumnNames="type_of_grading_id"
                                 baseTableName="student_exam_report"
                                 constraintName="fk_student_exam_report_type_of_grading_id"
                                 referencedColumnNames="id"
                                 referencedTableName="type_of_grading"/>

        <addForeignKeyConstraint baseColumnNames="batch_id"
                                 baseTableName="student_exam_report"
                                 constraintName="fk_student_exam_report_batch_id"
                                 referencedColumnNames="id"
                                 referencedTableName="batch"/>

        <addForeignKeyConstraint baseColumnNames="academicyear_id"
                                 baseTableName="student_exam_report"
                                 constraintName="fk_student_exam_report_academicyear_id"
                                 referencedColumnNames="id"
                                 referencedTableName="academic_year"/>

    </changeSet>
</databaseChangeLog>
