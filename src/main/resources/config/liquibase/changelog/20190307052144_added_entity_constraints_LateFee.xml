<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity LateFee.
    -->
    <changeSet id="20190307052144-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="college_id"
                                 baseTableName="late_fee"
                                 constraintName="fk_late_fee_college_id"
                                 referencedColumnNames="id"
                                 referencedTableName="college"/>

        <addForeignKeyConstraint baseColumnNames="branch_id"
                                 baseTableName="late_fee"
                                 constraintName="fk_late_fee_branch_id"
                                 referencedColumnNames="id"
                                 referencedTableName="branch"/>

        <addForeignKeyConstraint baseColumnNames="academic_year_id"
                                 baseTableName="late_fee"
                                 constraintName="fk_late_fee_academic_year_id"
                                 referencedColumnNames="id"
                                 referencedTableName="academic_year"/>

        <addForeignKeyConstraint baseColumnNames="term_id"
                                 baseTableName="late_fee"
                                 constraintName="fk_late_fee_term_id"
                                 referencedColumnNames="id"
                                 referencedTableName="term"/>

    </changeSet>
</databaseChangeLog>
